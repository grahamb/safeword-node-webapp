h1 Lunchgroup Heavy Industries
a(href='#')
	img(src='http://a0.twimg.com/images/dev/buttons/sign-in-with-twitter-l.png')
ul#word-list
  - for (var w in words)
		a(href='/words/' + words[w].word)
			- var class = words[w].wordoftheday ? 'wordoftheday' : 'word'
			li(id=words[w].word, class=class)
				span(href='/words/' + words[w].word)
					=words[w].word